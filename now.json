{
  "version": 2,
  "name": "minelev-buddy",
  "scope": "minelev",
  "alias": [
    "buddy.minelev.no"
  ],
  "env": {
    "NODE_ENV": "production",
    "BUDDY_API": "@minelev-buddy-api-url",
    "JWT_SECRET": "@minelev-jwt-secret",
    "PAPERTRAIL_HOSTNAME": "minelev",
    "PAPERTRAIL_HOST": "@tfk-papertrail-host",
    "PAPERTRAIL_PORT": "@tfk-papertrail-port"
  },
  "builds": [
    {
      "src": "/*.md",
      "use": "@now/md"
    },
    {
      "src": "/index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/README.html"
    },
    {
      "src": "/students",
      "dest": "/index.js"
    },
    {
      "src": "/students/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/teachers/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/classes/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/README.html"
    }
  ]
}
